{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module": "es6",
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/constant/*": [
        "src/constant/*"
      ],          
      "@/services/*": [
        "src/services/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ]     
    }
  },
  "include": [
    "./src/**/*"
  ]
}